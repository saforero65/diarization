[{"speaker":"SPEAKER_22","totalSegmentos":32,"segmentosSRT":[{"id":"346","startTime":2631.22,"endTime":2632.52,"text":"I'm home.","speaker":"SPEAKER_22"},{"id":"347","startTime":2633.46,"endTime":2634.55,"text":"Home.","speaker":"SPEAKER_22"},{"id":"814","startTime":5468.66,"endTime":5470.68,"text":"I brought some colleagues.","speaker":"SPEAKER_22"},{"id":"815","startTime":5470.76,"endTime":5472.78,"text":"We're fixing the place up.","speaker":"SPEAKER_22"},{"id":"816","startTime":5473.8,"endTime":5475.96,"text":"We replaced the roof tiles...","speaker":"SPEAKER_22"},{"id":"817","startTime":5476.03,"endTime":5478.16,"text":"so it wouldn't leak anymore.","speaker":"SPEAKER_22"},{"id":"818","startTime":5478.53,"endTime":5481.37,"text":"And we've painted Chairman Mao's portrait.","speaker":"SPEAKER_22"},{"id":"819","startTime":5483.61,"endTime":5485.27,"text":"Fengxia helped us.","speaker":"SPEAKER_22"},{"id":"867","startTime":5795.12,"endTime":5796.48,"text":"Dad, Mom...","speaker":"SPEAKER_22"},{"id":"868","startTime":5796.68,"endTime":5798.41,"text":"I'm taking Fengxia.","speaker":"SPEAKER_22"},{"id":"869","startTime":5798.69,"endTime":5800.65,"text":"I'll look after her.","speaker":"SPEAKER_22"},{"id":"889","startTime":5941.43,"endTime":5943.26,"text":"I'll be going.","speaker":"SPEAKER_22"},{"id":"891","startTime":5945.9,"endTime":5947.27,"text":"Never mind.","speaker":"SPEAKER_22"},{"id":"892","startTime":5949.94,"endTime":5952.74,"text":"Jiazhen, I know you don't want to see me.","speaker":"SPEAKER_22"},{"id":"893","startTime":5953.17,"endTime":5955.8,"text":"I'll leave my gift here and be gone.","speaker":"SPEAKER_22"},{"id":"907","startTime":6037.49,"endTime":6040.22,"text":"Lunch is all taken care of.","speaker":"SPEAKER_22"},{"id":"910","startTime":6074.9,"endTime":6077.66,"text":"Dad, Mom, we're having a baby.","speaker":"SPEAKER_22"},{"id":"934","startTime":6161.65,"endTime":6163.58,"text":"I had the photo framed.","speaker":"SPEAKER_22"},{"id":"974","startTime":6361.28,"endTime":6362.34,"text":"No.","speaker":"SPEAKER_22"},{"id":"975","startTime":6363.72,"endTime":6365.98,"text":"I just got an official notice.","speaker":"SPEAKER_22"},{"id":"1061","startTime":6718.31,"endTime":6721.1,"text":"Comrade, are there any doctors?","speaker":"SPEAKER_22"},{"id":"1089","startTime":6857.44,"endTime":6859.44,"text":"I was putting on a show.","speaker":"SPEAKER_22"},{"id":"1090","startTime":6860.18,"endTime":6861.7,"text":"We're not accusing you.","speaker":"SPEAKER_22"},{"id":"1091","startTime":6861.78,"endTime":6863.77,"text":"My wife's having a baby.","speaker":"SPEAKER_22"},{"id":"1092","startTime":6864.02,"endTime":6866.35,"text":"I wanted you here for her sake.","speaker":"SPEAKER_22"},{"id":"1206","startTime":7395.52,"endTime":7398.25,"text":"The only child I have left.","speaker":"SPEAKER_22"},{"id":"1231","startTime":7547.27,"endTime":7550.03,"text":"We should've bought them\non the way home.","speaker":"SPEAKER_22"},{"id":"1232","startTime":7550.54,"endTime":7552.66,"text":"Now we've got to carry them!","speaker":"SPEAKER_22"},{"id":"1233","startTime":7553.37,"endTime":7555.4,"text":"Careful! Don't drop them!","speaker":"SPEAKER_22"},{"id":"1266","startTime":7694.75,"endTime":7696.98,"text":"- I'll make dinner.\n- I'll help.","speaker":"SPEAKER_22"},{"id":"1267","startTime":7697.78,"endTime":7701.01,"text":"No, you're tired. You two have a rest.\nI'll do it.","speaker":"SPEAKER_22"},{"id":"1268","startTime":7701.95,"endTime":7704.51,"text":"Little Bun, behave yourself.","speaker":"SPEAKER_22"}]}]