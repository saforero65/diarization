[{"speaker":"SPEAKER_12","totalSegmentos":17,"segmentosSRT":[{"id":"281","startTime":1928.35,"endTime":1929.94,"text":"They're freezing.","speaker":"SPEAKER_12"},{"id":"282","startTime":1931.39,"endTime":1933.41,"text":"No one's looking after them?","speaker":"SPEAKER_12"},{"id":"293","startTime":1979.44,"endTime":1981.73,"text":"Was it from some dead officer?","speaker":"SPEAKER_12"},{"id":"298","startTime":2027.28,"endTime":2030.15,"text":"Lao Quan, the place is deserted.","speaker":"SPEAKER_12"},{"id":"299","startTime":2030.82,"endTime":2033.12,"text":"I'm still dizzy from drinking!","speaker":"SPEAKER_12"},{"id":"301","startTime":2038.93,"endTime":2040.59,"text":"They've all run off.","speaker":"SPEAKER_12"},{"id":"302","startTime":2040.66,"endTime":2042.13,"text":"How could that be?","speaker":"SPEAKER_12"},{"id":"303","startTime":2042.2,"endTime":2043.96,"text":"The Reds must be on the attack.","speaker":"SPEAKER_12"},{"id":"304","startTime":2044.03,"endTime":2045.13,"text":"What?","speaker":"SPEAKER_12"},{"id":"305","startTime":2049,"endTime":2051.34,"text":"- Shouldn't we run, too?\n- Where to?","speaker":"SPEAKER_12"},{"id":"309","startTime":2057.48,"endTime":2058.81,"text":"Run now...","speaker":"SPEAKER_12"},{"id":"318","startTime":2081.47,"endTime":2082.53,"text":"How?","speaker":"SPEAKER_12"},{"id":"320","startTime":2092.35,"endTime":2094.51,"text":"Don't reach for your waist!","speaker":"SPEAKER_12"},{"id":"321","startTime":2094.58,"endTime":2096.57,"text":"Do that and you're dead.","speaker":"SPEAKER_12"},{"id":"322","startTime":2097.32,"endTime":2101.65,"text":"The Reds treat POWs well.\nI've been one before.","speaker":"SPEAKER_12"},{"id":"323","startTime":2102.02,"endTime":2104.89,"text":"Just put those hands up.","speaker":"SPEAKER_12"},{"id":"333","startTime":2234.19,"endTime":2237.82,"text":"The coat's from my brother's unit.","speaker":"SPEAKER_12"}]}]