[{"speaker":"SPEAKER_07","totalSegmentos":31,"segmentosSRT":[{"id":"173","startTime":1087.51,"endTime":1090.64,"text":"And the debt is hereby settled.","speaker":"SPEAKER_07"},{"id":"281","startTime":1928.35,"endTime":1929.94,"text":"They're freezing.","speaker":"SPEAKER_07"},{"id":"282","startTime":1931.39,"endTime":1933.41,"text":"No one's looking after them?","speaker":"SPEAKER_07"},{"id":"293","startTime":1979.44,"endTime":1981.73,"text":"Was it from some dead officer?","speaker":"SPEAKER_07"},{"id":"298","startTime":2027.28,"endTime":2030.15,"text":"Lao Quan, the place is deserted.","speaker":"SPEAKER_07"},{"id":"299","startTime":2030.82,"endTime":2033.12,"text":"I'm still dizzy from drinking!","speaker":"SPEAKER_07"},{"id":"301","startTime":2038.93,"endTime":2040.59,"text":"They've all run off.","speaker":"SPEAKER_07"},{"id":"302","startTime":2040.66,"endTime":2042.13,"text":"How could that be?","speaker":"SPEAKER_07"},{"id":"303","startTime":2042.2,"endTime":2043.96,"text":"The Reds must be on the attack.","speaker":"SPEAKER_07"},{"id":"309","startTime":2057.48,"endTime":2058.81,"text":"Run now...","speaker":"SPEAKER_07"},{"id":"310","startTime":2059.11,"endTime":2061.38,"text":"and you'll be shot.","speaker":"SPEAKER_07"},{"id":"311","startTime":2064.09,"endTime":2066.55,"text":"Better to wait and become a POW.","speaker":"SPEAKER_07"},{"id":"314","startTime":2072.53,"endTime":2073.72,"text":"That's right.","speaker":"SPEAKER_07"},{"id":"315","startTime":2073.8,"endTime":2076.39,"text":"You want to go home, don't run.","speaker":"SPEAKER_07"},{"id":"318","startTime":2081.47,"endTime":2082.53,"text":"How?","speaker":"SPEAKER_07"},{"id":"320","startTime":2092.35,"endTime":2094.51,"text":"Don't reach for your waist!","speaker":"SPEAKER_07"},{"id":"321","startTime":2094.58,"endTime":2096.57,"text":"Do that and you're dead.","speaker":"SPEAKER_07"},{"id":"322","startTime":2097.32,"endTime":2101.65,"text":"The Reds treat POWs well.\nI've been one before.","speaker":"SPEAKER_07"},{"id":"323","startTime":2102.02,"endTime":2104.89,"text":"Just put those hands up.","speaker":"SPEAKER_07"},{"id":"331","startTime":2226.38,"endTime":2228.85,"text":"Where did you find this coat?","speaker":"SPEAKER_07"},{"id":"333","startTime":2234.19,"endTime":2237.82,"text":"The coat's from my brother's unit.","speaker":"SPEAKER_07"},{"id":"460","startTime":3199.19,"endTime":3201.28,"text":"There'll be meat, fish.","speaker":"SPEAKER_07"},{"id":"461","startTime":3203.49,"endTime":3205.76,"text":"Nothing else to contribute?","speaker":"SPEAKER_07"},{"id":"462","startTime":3206.39,"endTime":3207.92,"text":"Yes, there is!","speaker":"SPEAKER_07"},{"id":"463","startTime":3213.1,"endTime":3214.69,"text":"But that's wood.","speaker":"SPEAKER_07"},{"id":"575","startTime":4067.96,"endTime":4069.69,"text":"We've smelted our steel...","speaker":"SPEAKER_07"},{"id":"576","startTime":4069.76,"endTime":4071.99,"text":"and reached our production target.","speaker":"SPEAKER_07"},{"id":"577","startTime":4072.06,"endTime":4074.5,"text":"We're off to report the good news.","speaker":"SPEAKER_07"},{"id":"594","startTime":4188.41,"endTime":4189.44,"text":"What's up?","speaker":"SPEAKER_07"},{"id":"764","startTime":5192.48,"endTime":5194.45,"text":"Comrade Wan Erxi's here.","speaker":"SPEAKER_07"},{"id":"862","startTime":5763.05,"endTime":5765.58,"text":"First, pay respects to Chairman Mao.","speaker":"SPEAKER_07"}]}]