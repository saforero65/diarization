[{"speaker":"SPEAKER_00","totalSegmentos":17,"segmentosSRT":[{"id":"281","startTime":1928.35,"endTime":1929.94,"text":"They're freezing.","speaker":"SPEAKER_00"},{"id":"282","startTime":1931.39,"endTime":1933.41,"text":"No one's looking after them?","speaker":"SPEAKER_00"},{"id":"293","startTime":1979.44,"endTime":1981.73,"text":"Was it from some dead officer?","speaker":"SPEAKER_00"},{"id":"306","startTime":2051.54,"endTime":2052.6,"text":"Home!","speaker":"SPEAKER_00"},{"id":"307","startTime":2052.68,"endTime":2053.77,"text":"Home?","speaker":"SPEAKER_00"},{"id":"308","startTime":2054.84,"endTime":2056.57,"text":"Wait right here.","speaker":"SPEAKER_00"},{"id":"309","startTime":2057.48,"endTime":2058.81,"text":"Run now...","speaker":"SPEAKER_00"},{"id":"310","startTime":2059.11,"endTime":2061.38,"text":"and you'll be shot.","speaker":"SPEAKER_00"},{"id":"311","startTime":2064.09,"endTime":2066.55,"text":"Better to wait and become a POW.","speaker":"SPEAKER_00"},{"id":"314","startTime":2072.53,"endTime":2073.72,"text":"That's right.","speaker":"SPEAKER_00"},{"id":"315","startTime":2073.8,"endTime":2076.39,"text":"You want to go home, don't run.","speaker":"SPEAKER_00"},{"id":"318","startTime":2081.47,"endTime":2082.53,"text":"How?","speaker":"SPEAKER_00"},{"id":"319","startTime":2088.04,"endTime":2089.94,"text":"This will do the trick?","speaker":"SPEAKER_00"},{"id":"320","startTime":2092.35,"endTime":2094.51,"text":"Don't reach for your waist!","speaker":"SPEAKER_00"},{"id":"321","startTime":2094.58,"endTime":2096.57,"text":"Do that and you're dead.","speaker":"SPEAKER_00"},{"id":"322","startTime":2097.32,"endTime":2101.65,"text":"The Reds treat POWs well.\nI've been one before.","speaker":"SPEAKER_00"},{"id":"323","startTime":2102.02,"endTime":2104.89,"text":"Just put those hands up.","speaker":"SPEAKER_00"}]}]