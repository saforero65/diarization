[{"speaker":"SPEAKER_12","totalSegmentos":32,"segmentosSRT":[{"id":"42","startTime":338.13,"endTime":339.46,"text":"Take care.","speaker":"SPEAKER_12"},{"id":"43","startTime":340.26,"endTime":342.32,"text":"Take care, Master.","speaker":"SPEAKER_12"},{"id":"44","startTime":365.69,"endTime":367.35,"text":"Welcome home, sir.","speaker":"SPEAKER_12"},{"id":"45","startTime":368.56,"endTime":369.62,"text":"Dad!","speaker":"SPEAKER_12"},{"id":"46","startTime":373.16,"endTime":375.66,"text":"Degenerate! Another night gambling!","speaker":"SPEAKER_12"},{"id":"145","startTime":900.62,"endTime":903.12,"text":"Not that we'd accept it as ante.","speaker":"SPEAKER_12"},{"id":"631","startTime":4387.04,"endTime":4388.67,"text":"It's Fugui's son?","speaker":"SPEAKER_12"},{"id":"632","startTime":4389.75,"endTime":4391.65,"text":"District Chief arrived after dark.","speaker":"SPEAKER_12"},{"id":"633","startTime":4391.71,"endTime":4396.08,"text":"He backed up,\nand his jeep knocked the wall down.","speaker":"SPEAKER_12"},{"id":"634","startTime":4396.15,"endTime":4399.02,"text":"Youqing was sleeping on the other side.","speaker":"SPEAKER_12"},{"id":"635","startTime":4399.56,"endTime":4401.85,"text":"When the wall collapsed...","speaker":"SPEAKER_12"},{"id":"636","startTime":4402.33,"endTime":4404.16,"text":"he didn't have a chance.","speaker":"SPEAKER_12"},{"id":"637","startTime":4404.23,"endTime":4406.56,"text":"The District Chief was hurt, too.","speaker":"SPEAKER_12"},{"id":"638","startTime":4407.8,"endTime":4410.13,"text":"He hadn't slept for three days.","speaker":"SPEAKER_12"},{"id":"639","startTime":4436.73,"endTime":4438.42,"text":"Fugui, listen to me.","speaker":"SPEAKER_12"},{"id":"640","startTime":4438.49,"endTime":4442.43,"text":"We have to try and bear it,\neven in the worst of times.","speaker":"SPEAKER_12"},{"id":"831","startTime":5533.12,"endTime":5534.78,"text":"Please come again.","speaker":"SPEAKER_12"},{"id":"832","startTime":5542.36,"endTime":5545.49,"text":"What do you think about Fengxia and me?","speaker":"SPEAKER_12"},{"id":"916","startTime":6097.49,"endTime":6098.68,"text":"Cheers!","speaker":"SPEAKER_12"},{"id":"1058","startTime":6702.52,"endTime":6704.55,"text":"They've been locked away.","speaker":"SPEAKER_12"},{"id":"1059","startTime":6704.93,"endTime":6708.02,"text":"The Red Guards are in control!","speaker":"SPEAKER_12"},{"id":"1150","startTime":7151.87,"endTime":7153.46,"text":"- Have a look.\n- Not yet!","speaker":"SPEAKER_12"},{"id":"1151","startTime":7153.54,"endTime":7155.53,"text":"Please, I'm the father!","speaker":"SPEAKER_12"},{"id":"1155","startTime":7168.29,"endTime":7169.72,"text":"Is it a boy?","speaker":"SPEAKER_12"},{"id":"1156","startTime":7170.66,"endTime":7172.28,"text":"Yes, 7.9 pounds!","speaker":"SPEAKER_12"},{"id":"1157","startTime":7172.66,"endTime":7175.29,"text":"- How is he?\n- They're both fine!","speaker":"SPEAKER_12"},{"id":"1175","startTime":7259.18,"endTime":7260.81,"text":"Why'd he do that?","speaker":"SPEAKER_12"},{"id":"1176","startTime":7260.88,"endTime":7264.18,"text":"He's been locked up without food.\nHe scoffed those buns.","speaker":"SPEAKER_12"},{"id":"1177","startTime":7265.75,"endTime":7267.38,"text":"Drink it slowly.","speaker":"SPEAKER_12"},{"id":"1178","startTime":7270.36,"endTime":7271.58,"text":"Help us!","speaker":"SPEAKER_12"},{"id":"1179","startTime":7271.66,"endTime":7275.25,"text":"She's bleeding! We don't know what to do.","speaker":"SPEAKER_12"},{"id":"1180","startTime":7275.33,"endTime":7276.52,"text":"Quick!","speaker":"SPEAKER_12"}]}]