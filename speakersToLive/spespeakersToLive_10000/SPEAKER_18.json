[{"speaker":"SPEAKER_18","totalSegmentos":31,"segmentosSRT":[{"id":"171","startTime":1082.71,"endTime":1084.5,"text":"The Xu family home...","speaker":"SPEAKER_18"},{"id":"172","startTime":1084.91,"endTime":1087.1,"text":"goes to Long'er.","speaker":"SPEAKER_18"},{"id":"173","startTime":1087.51,"endTime":1090.64,"text":"And the debt is hereby settled.","speaker":"SPEAKER_18"},{"id":"260","startTime":1778.13,"endTime":1779.47,"text":"Besides...","speaker":"SPEAKER_18"},{"id":"261","startTime":1780.27,"endTime":1782.86,"text":"troops for both sides are everywhere.","speaker":"SPEAKER_18"},{"id":"264","startTime":1789.68,"endTime":1792.41,"text":"Forget it. Try to save your own skin.","speaker":"SPEAKER_18"},{"id":"265","startTime":1802.86,"endTime":1804.26,"text":"As for me...","speaker":"SPEAKER_18"},{"id":"266","startTime":1806.2,"endTime":1810.6,"text":"if I weren't looking for my brother,\nI wouldn't be wearing this uniform.","speaker":"SPEAKER_18"},{"id":"267","startTime":1836.49,"endTime":1839.86,"text":"Chunsheng, you really are mad.","speaker":"SPEAKER_18"},{"id":"278","startTime":1914.07,"endTime":1916.04,"text":"What a bloodbath.","speaker":"SPEAKER_18"},{"id":"279","startTime":1923.75,"endTime":1925.41,"text":"What's that sound?","speaker":"SPEAKER_18"},{"id":"280","startTime":1926.15,"endTime":1927.55,"text":"The wounded.","speaker":"SPEAKER_18"},{"id":"281","startTime":1928.35,"endTime":1929.94,"text":"They're freezing.","speaker":"SPEAKER_18"},{"id":"282","startTime":1931.39,"endTime":1933.41,"text":"No one's looking after them?","speaker":"SPEAKER_18"},{"id":"283","startTime":1933.49,"endTime":1934.98,"text":"No one's left.","speaker":"SPEAKER_18"},{"id":"284","startTime":1936.09,"endTime":1939.12,"text":"Must be tens of thousands\nof us trapped here.","speaker":"SPEAKER_18"},{"id":"293","startTime":1979.44,"endTime":1981.73,"text":"Was it from some dead officer?","speaker":"SPEAKER_18"},{"id":"296","startTime":1997.19,"endTime":1999.09,"text":"We must help each other.","speaker":"SPEAKER_18"},{"id":"297","startTime":1999.42,"endTime":2001.82,"text":"Besides, we're from the same town.","speaker":"SPEAKER_18"},{"id":"306","startTime":2051.54,"endTime":2052.6,"text":"Home!","speaker":"SPEAKER_18"},{"id":"307","startTime":2052.68,"endTime":2053.77,"text":"Home?","speaker":"SPEAKER_18"},{"id":"308","startTime":2054.84,"endTime":2056.57,"text":"Wait right here.","speaker":"SPEAKER_18"},{"id":"309","startTime":2057.48,"endTime":2058.81,"text":"Run now...","speaker":"SPEAKER_18"},{"id":"310","startTime":2059.11,"endTime":2061.38,"text":"and you'll be shot.","speaker":"SPEAKER_18"},{"id":"314","startTime":2072.53,"endTime":2073.72,"text":"That's right.","speaker":"SPEAKER_18"},{"id":"315","startTime":2073.8,"endTime":2076.39,"text":"You want to go home, don't run.","speaker":"SPEAKER_18"},{"id":"320","startTime":2092.35,"endTime":2094.51,"text":"Don't reach for your waist!","speaker":"SPEAKER_18"},{"id":"321","startTime":2094.58,"endTime":2096.57,"text":"Do that and you're dead.","speaker":"SPEAKER_18"},{"id":"322","startTime":2097.32,"endTime":2101.65,"text":"The Reds treat POWs well.\nI've been one before.","speaker":"SPEAKER_18"},{"id":"323","startTime":2102.02,"endTime":2104.89,"text":"Just put those hands up.","speaker":"SPEAKER_18"},{"id":"963","startTime":6323.18,"endTime":6326.31,"text":"It's all in this account. Take it.","speaker":"SPEAKER_18"}]}]