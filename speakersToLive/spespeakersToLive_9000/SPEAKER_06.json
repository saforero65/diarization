[{"speaker":"SPEAKER_06","totalSegmentos":71,"segmentosSRT":[{"id":"29","startTime":223.38,"endTime":226.08,"text":"Business is good, Mr. Xie.","speaker":"SPEAKER_06"},{"id":"86","startTime":544,"endTime":545.66,"text":"My luck's changed.","speaker":"SPEAKER_06"},{"id":"87","startTime":547.34,"endTime":550.4,"text":"Write that down. Another round.","speaker":"SPEAKER_06"},{"id":"118","startTime":775.17,"endTime":777.69,"text":"You've lost everything.","speaker":"SPEAKER_06"},{"id":"119","startTime":778.2,"endTime":780.43,"text":"What do you mean?","speaker":"SPEAKER_06"},{"id":"129","startTime":845.37,"endTime":847.67,"text":"You can't just win and walk away.","speaker":"SPEAKER_06"},{"id":"130","startTime":847.74,"endTime":850.33,"text":"The game's not over yet!","speaker":"SPEAKER_06"},{"id":"268","startTime":1841.03,"endTime":1843.62,"text":"I'd die happy if I could drive.","speaker":"SPEAKER_06"},{"id":"269","startTime":1844.77,"endTime":1846.17,"text":"As for me...","speaker":"SPEAKER_06"},{"id":"270","startTime":1846.9,"endTime":1850.57,"text":"I want to live. There's nothing like family.","speaker":"SPEAKER_06"},{"id":"316","startTime":2076.73,"endTime":2078.17,"text":"They won't shoot us?","speaker":"SPEAKER_06"},{"id":"317","startTime":2078.23,"endTime":2081.4,"text":"Not as long as you've got your hands up.","speaker":"SPEAKER_06"},{"id":"342","startTime":2564.39,"endTime":2565.65,"text":"Fengxia!","speaker":"SPEAKER_06"},{"id":"357","startTime":2722.44,"endTime":2724.17,"text":"I thought of you...","speaker":"SPEAKER_06"},{"id":"358","startTime":2725.01,"endTime":2727.11,"text":"all the time and worried...","speaker":"SPEAKER_06"},{"id":"359","startTime":2727.85,"endTime":2729.82,"text":"how you were getting by.","speaker":"SPEAKER_06"},{"id":"360","startTime":2730.32,"endTime":2733.31,"text":"The new government\npaid for mother's burial...","speaker":"SPEAKER_06"},{"id":"384","startTime":2830.92,"endTime":2833.05,"text":"You participated in the revolution.","speaker":"SPEAKER_06"},{"id":"385","startTime":2833.12,"endTime":2835.45,"text":"I just dragged a cannon and sang opera.","speaker":"SPEAKER_06"},{"id":"400","startTime":2888.54,"endTime":2889.94,"text":"He burnt it?","speaker":"SPEAKER_06"},{"id":"432","startTime":3074.13,"endTime":3076.06,"text":"Better dry it out fast.","speaker":"SPEAKER_06"},{"id":"466","startTime":3224.58,"endTime":3226.07,"text":"It can't be worth much.","speaker":"SPEAKER_06"},{"id":"598","startTime":4198.62,"endTime":4200.92,"text":"Pack a lunchbox. I'll wake him.","speaker":"SPEAKER_06"},{"id":"599","startTime":4201.76,"endTime":4204.92,"text":"He's not going anywhere.\nHe hasn't slept in days.","speaker":"SPEAKER_06"},{"id":"600","startTime":4205.8,"endTime":4207.39,"text":"Leave him alone.","speaker":"SPEAKER_06"},{"id":"601","startTime":4208.5,"endTime":4211.9,"text":"- The District Chief is coming.\n- So?","speaker":"SPEAKER_06"},{"id":"603","startTime":4216.14,"endTime":4220.04,"text":"Smelting steel is a duty.\nWe can't be politically backward.","speaker":"SPEAKER_06"},{"id":"604","startTime":4220.11,"endTime":4222.81,"text":"Mr. Niu praised us in front of everyone.","speaker":"SPEAKER_06"},{"id":"605","startTime":4226.32,"endTime":4228.72,"text":"Youqing has to go.","speaker":"SPEAKER_06"},{"id":"606","startTime":4232.92,"endTime":4235.82,"text":"Wake up.","speaker":"SPEAKER_06"},{"id":"618","startTime":4293.52,"endTime":4295.68,"text":"- Do you like dumplings?\n- Yes.","speaker":"SPEAKER_06"},{"id":"619","startTime":4296.25,"endTime":4298.31,"text":"- Do you like meat?\n- Yes.","speaker":"SPEAKER_06"},{"id":"761","startTime":5168.89,"endTime":5172.49,"text":"Shall we invite him over?\nFengxia can check him out.","speaker":"SPEAKER_06"},{"id":"766","startTime":5202.02,"endTime":5203.99,"text":"Fengxia's mom and dad.","speaker":"SPEAKER_06"},{"id":"793","startTime":5346.5,"endTime":5348.66,"text":"Anyway, it's up to Fengxia.","speaker":"SPEAKER_06"},{"id":"854","startTime":5703.59,"endTime":5706.42,"text":"At least it's loud enough for her to hear.","speaker":"SPEAKER_06"},{"id":"877","startTime":5878.47,"endTime":5880.37,"text":"Jiazhen, make some tea.","speaker":"SPEAKER_06"},{"id":"878","startTime":5890.61,"endTime":5892.58,"text":"Isn't the tea ready yet?","speaker":"SPEAKER_06"},{"id":"879","startTime":5892.71,"endTime":5894.84,"text":"Chunsheng's been here for ages.","speaker":"SPEAKER_06"},{"id":"890","startTime":5943.87,"endTime":5945.83,"text":"Don't go. The tea's ready.","speaker":"SPEAKER_06"},{"id":"930","startTime":6145.87,"endTime":6148.43,"text":"But things are better now.","speaker":"SPEAKER_06"},{"id":"931","startTime":6148.87,"endTime":6151.34,"text":"And will be better for your child.","speaker":"SPEAKER_06"},{"id":"947","startTime":6219.31,"endTime":6220.67,"text":"Of course.","speaker":"SPEAKER_06"},{"id":"969","startTime":6347.53,"endTime":6349.87,"text":"Yesterday, my wife killed herself.","speaker":"SPEAKER_06"},{"id":"1052","startTime":6689.64,"endTime":6691.47,"text":"Who examined Fengxia?","speaker":"SPEAKER_06"},{"id":"1053","startTime":6691.55,"endTime":6692.77,"text":"They did.","speaker":"SPEAKER_06"},{"id":"1054","startTime":6692.85,"endTime":6695.11,"text":"You haven't seen any doctors?","speaker":"SPEAKER_06"},{"id":"1060","startTime":6709.36,"endTime":6710.76,"text":"No doctors?","speaker":"SPEAKER_06"},{"id":"1061","startTime":6718.31,"endTime":6721.1,"text":"Comrade, are there any doctors?","speaker":"SPEAKER_06"},{"id":"1069","startTime":6748.37,"endTime":6750.5,"text":"We need an experienced doctor.","speaker":"SPEAKER_06"},{"id":"1070","startTime":6750.57,"endTime":6753.77,"text":"Even if he only stands by, we would relax.","speaker":"SPEAKER_06"},{"id":"1101","startTime":6945.73,"endTime":6947.2,"text":"Where's Erxi?","speaker":"SPEAKER_06"},{"id":"1103","startTime":6949.3,"endTime":6950.6,"text":"How's Fengxia?","speaker":"SPEAKER_06"},{"id":"1114","startTime":6998.12,"endTime":6999.71,"text":"Why not use this time...","speaker":"SPEAKER_06"},{"id":"1115","startTime":6999.79,"endTime":7002.12,"text":"to think of a name for the kid?","speaker":"SPEAKER_06"},{"id":"1121","startTime":7029.38,"endTime":7031.25,"text":"You just say anything!","speaker":"SPEAKER_06"},{"id":"1122","startTime":7031.55,"endTime":7033.25,"text":"That's not a name!","speaker":"SPEAKER_06"},{"id":"1210","startTime":7473.59,"endTime":7474.79,"text":"Is it?","speaker":"SPEAKER_06"},{"id":"1211","startTime":7474.86,"endTime":7476.29,"text":"I'll fix it.","speaker":"SPEAKER_06"},{"id":"1221","startTime":7509.9,"endTime":7511.29,"text":"Little Bun...","speaker":"SPEAKER_06"},{"id":"1222","startTime":7511.36,"endTime":7512.92,"text":"give me a hand.","speaker":"SPEAKER_06"},{"id":"1233","startTime":7553.37,"endTime":7555.4,"text":"Careful! Don't drop them!","speaker":"SPEAKER_06"},{"id":"1234","startTime":7556.24,"endTime":7557.77,"text":"Are you happy?","speaker":"SPEAKER_06"},{"id":"1236","startTime":7563.08,"endTime":7564.71,"text":"Where will they live?","speaker":"SPEAKER_06"},{"id":"1237","startTime":7564.78,"endTime":7567.72,"text":"I'll find a home for them when we get back.","speaker":"SPEAKER_06"},{"id":"1249","startTime":7620.24,"endTime":7622.5,"text":"everything would've been fine.","speaker":"SPEAKER_06"},{"id":"1250","startTime":7623.38,"endTime":7625.74,"text":"He could have saved our Fengxia.","speaker":"SPEAKER_06"},{"id":"1251","startTime":7626.21,"endTime":7628.37,"text":"He's a professor, after all.","speaker":"SPEAKER_06"},{"id":"1259","startTime":7651.67,"endTime":7653.43,"text":"It's much too much.","speaker":"SPEAKER_06"},{"id":"1270","startTime":7710.53,"endTime":7711.89,"text":"Let's see.","speaker":"SPEAKER_06"},{"id":"1281","startTime":7758.91,"endTime":7761.54,"text":"and the geese will turn into sheep...","speaker":"SPEAKER_06"}]}]