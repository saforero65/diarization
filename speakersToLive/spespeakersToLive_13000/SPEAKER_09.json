[{"speaker":"SPEAKER_09","totalSegmentos":56,"segmentosSRT":[{"id":"171","startTime":1082.71,"endTime":1084.5,"text":"The Xu family home...","speaker":"SPEAKER_09"},{"id":"172","startTime":1084.91,"endTime":1087.1,"text":"goes to Long'er.","speaker":"SPEAKER_09"},{"id":"174","startTime":1091.65,"endTime":1093.45,"text":"Both sides must sign.","speaker":"SPEAKER_09"},{"id":"256","startTime":1754.74,"endTime":1756.77,"text":"Might never go home again.","speaker":"SPEAKER_09"},{"id":"257","startTime":1767.26,"endTime":1769.22,"text":"What are you looking at?","speaker":"SPEAKER_09"},{"id":"258","startTime":1771.09,"endTime":1772.89,"text":"Thinking of escaping?","speaker":"SPEAKER_09"},{"id":"259","startTime":1774.36,"endTime":1777.13,"text":"The Nationalists will shoot you.","speaker":"SPEAKER_09"},{"id":"260","startTime":1778.13,"endTime":1779.47,"text":"Besides...","speaker":"SPEAKER_09"},{"id":"261","startTime":1780.27,"endTime":1782.86,"text":"troops for both sides are everywhere.","speaker":"SPEAKER_09"},{"id":"262","startTime":1783.21,"endTime":1785.57,"text":"If you don't fight the Reds,\nyou fight the KMT.","speaker":"SPEAKER_09"},{"id":"263","startTime":1785.64,"endTime":1789.17,"text":"But who will look after my wife,\nmy kids, my old mother?","speaker":"SPEAKER_09"},{"id":"264","startTime":1789.68,"endTime":1792.41,"text":"Forget it. Try to save your own skin.","speaker":"SPEAKER_09"},{"id":"265","startTime":1802.86,"endTime":1804.26,"text":"As for me...","speaker":"SPEAKER_09"},{"id":"266","startTime":1806.2,"endTime":1810.6,"text":"if I weren't looking for my brother,\nI wouldn't be wearing this uniform.","speaker":"SPEAKER_09"},{"id":"267","startTime":1836.49,"endTime":1839.86,"text":"Chunsheng, you really are mad.","speaker":"SPEAKER_09"},{"id":"278","startTime":1914.07,"endTime":1916.04,"text":"What a bloodbath.","speaker":"SPEAKER_09"},{"id":"279","startTime":1923.75,"endTime":1925.41,"text":"What's that sound?","speaker":"SPEAKER_09"},{"id":"280","startTime":1926.15,"endTime":1927.55,"text":"The wounded.","speaker":"SPEAKER_09"},{"id":"283","startTime":1933.49,"endTime":1934.98,"text":"No one's left.","speaker":"SPEAKER_09"},{"id":"284","startTime":1936.09,"endTime":1939.12,"text":"Must be tens of thousands\nof us trapped here.","speaker":"SPEAKER_09"},{"id":"290","startTime":1966.02,"endTime":1969.42,"text":"Put it on. We must survive.","speaker":"SPEAKER_09"},{"id":"291","startTime":1970.96,"endTime":1973.09,"text":"Lao Quan, have a drink.","speaker":"SPEAKER_09"},{"id":"296","startTime":1997.19,"endTime":1999.09,"text":"We must help each other.","speaker":"SPEAKER_09"},{"id":"297","startTime":1999.42,"endTime":2001.82,"text":"Besides, we're from the same town.","speaker":"SPEAKER_09"},{"id":"306","startTime":2051.54,"endTime":2052.6,"text":"Home!","speaker":"SPEAKER_09"},{"id":"307","startTime":2052.68,"endTime":2053.77,"text":"Home?","speaker":"SPEAKER_09"},{"id":"308","startTime":2054.84,"endTime":2056.57,"text":"Wait right here.","speaker":"SPEAKER_09"},{"id":"312","startTime":2066.86,"endTime":2070.02,"text":"The Reds send POWs home.","speaker":"SPEAKER_09"},{"id":"319","startTime":2088.04,"endTime":2089.94,"text":"This will do the trick?","speaker":"SPEAKER_09"},{"id":"673","startTime":4739.5,"endTime":4741.16,"text":"I meant to call...","speaker":"SPEAKER_09"},{"id":"675","startTime":4746.7,"endTime":4747.93,"text":"Fugui...","speaker":"SPEAKER_09"},{"id":"676","startTime":4749.71,"endTime":4751.7,"text":"how could it be your son?","speaker":"SPEAKER_09"},{"id":"686","startTime":4797.55,"endTime":4799.49,"text":"Take this for now.","speaker":"SPEAKER_09"},{"id":"702","startTime":4856.91,"endTime":4858.54,"text":"I owe you a life.","speaker":"SPEAKER_09"},{"id":"703","startTime":4861.15,"endTime":4862.74,"text":"I will remember.","speaker":"SPEAKER_09"},{"id":"875","startTime":5869.09,"endTime":5872.15,"text":"But there were so many people,\nI didn't come in.","speaker":"SPEAKER_09"},{"id":"950","startTime":6246.27,"endTime":6247.43,"text":"Fugui!","speaker":"SPEAKER_09"},{"id":"951","startTime":6251.37,"endTime":6252.77,"text":"Who's there?","speaker":"SPEAKER_09"},{"id":"952","startTime":6252.84,"endTime":6254.21,"text":"Chunsheng.","speaker":"SPEAKER_09"},{"id":"958","startTime":6273.76,"endTime":6275.82,"text":"It'll just take a minute.","speaker":"SPEAKER_09"},{"id":"959","startTime":6308.36,"endTime":6309.49,"text":"Well?","speaker":"SPEAKER_09"},{"id":"960","startTime":6310.1,"endTime":6311.33,"text":"Fugui...","speaker":"SPEAKER_09"},{"id":"961","startTime":6312.33,"endTime":6315.06,"text":"I've always felt terrible about Youqing.","speaker":"SPEAKER_09"},{"id":"962","startTime":6317.34,"endTime":6321.3,"text":"I've often tried to give you money.\nYou'd never accept it.","speaker":"SPEAKER_09"},{"id":"963","startTime":6323.18,"endTime":6326.31,"text":"It's all in this account. Take it.","speaker":"SPEAKER_09"},{"id":"965","startTime":6333.39,"endTime":6334.82,"text":"Take this...","speaker":"SPEAKER_09"},{"id":"966","startTime":6336.22,"endTime":6338.42,"text":"and my heart will be at ease.","speaker":"SPEAKER_09"},{"id":"969","startTime":6347.53,"endTime":6349.87,"text":"Yesterday, my wife killed herself.","speaker":"SPEAKER_09"},{"id":"974","startTime":6361.28,"endTime":6362.34,"text":"No.","speaker":"SPEAKER_09"},{"id":"975","startTime":6363.72,"endTime":6365.98,"text":"I just got an official notice.","speaker":"SPEAKER_09"},{"id":"990","startTime":6431.62,"endTime":6433.45,"text":"I don't think I can.","speaker":"SPEAKER_09"},{"id":"995","startTime":6450.1,"endTime":6452.04,"text":"I snuck out just now...","speaker":"SPEAKER_09"},{"id":"996","startTime":6452.11,"endTime":6453.9,"text":"and better get back.","speaker":"SPEAKER_09"},{"id":"997","startTime":6461.38,"endTime":6464.35,"text":"Chunsheng, remember...","speaker":"SPEAKER_09"},{"id":"1093","startTime":6870.29,"endTime":6871.88,"text":"No, it's better if I wear it.","speaker":"SPEAKER_09"},{"id":"1094","startTime":6871.96,"endTime":6873.48,"text":"Let me wear it!","speaker":"SPEAKER_09"}]}]