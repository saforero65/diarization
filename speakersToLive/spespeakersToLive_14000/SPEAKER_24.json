[{"speaker":"SPEAKER_24","totalSegmentos":26,"segmentosSRT":[{"id":"171","startTime":1082.71,"endTime":1084.5,"text":"The Xu family home...","speaker":"SPEAKER_24"},{"id":"172","startTime":1084.91,"endTime":1087.1,"text":"goes to Long'er.","speaker":"SPEAKER_24"},{"id":"173","startTime":1087.51,"endTime":1090.64,"text":"And the debt is hereby settled.","speaker":"SPEAKER_24"},{"id":"278","startTime":1914.07,"endTime":1916.04,"text":"What a bloodbath.","speaker":"SPEAKER_24"},{"id":"281","startTime":1928.35,"endTime":1929.94,"text":"They're freezing.","speaker":"SPEAKER_24"},{"id":"282","startTime":1931.39,"endTime":1933.41,"text":"No one's looking after them?","speaker":"SPEAKER_24"},{"id":"293","startTime":1979.44,"endTime":1981.73,"text":"Was it from some dead officer?","speaker":"SPEAKER_24"},{"id":"299","startTime":2030.82,"endTime":2033.12,"text":"I'm still dizzy from drinking!","speaker":"SPEAKER_24"},{"id":"302","startTime":2040.66,"endTime":2042.13,"text":"How could that be?","speaker":"SPEAKER_24"},{"id":"303","startTime":2042.2,"endTime":2043.96,"text":"The Reds must be on the attack.","speaker":"SPEAKER_24"},{"id":"306","startTime":2051.54,"endTime":2052.6,"text":"Home!","speaker":"SPEAKER_24"},{"id":"307","startTime":2052.68,"endTime":2053.77,"text":"Home?","speaker":"SPEAKER_24"},{"id":"308","startTime":2054.84,"endTime":2056.57,"text":"Wait right here.","speaker":"SPEAKER_24"},{"id":"309","startTime":2057.48,"endTime":2058.81,"text":"Run now...","speaker":"SPEAKER_24"},{"id":"310","startTime":2059.11,"endTime":2061.38,"text":"and you'll be shot.","speaker":"SPEAKER_24"},{"id":"311","startTime":2064.09,"endTime":2066.55,"text":"Better to wait and become a POW.","speaker":"SPEAKER_24"},{"id":"312","startTime":2066.86,"endTime":2070.02,"text":"The Reds send POWs home.","speaker":"SPEAKER_24"},{"id":"314","startTime":2072.53,"endTime":2073.72,"text":"That's right.","speaker":"SPEAKER_24"},{"id":"315","startTime":2073.8,"endTime":2076.39,"text":"You want to go home, don't run.","speaker":"SPEAKER_24"},{"id":"318","startTime":2081.47,"endTime":2082.53,"text":"How?","speaker":"SPEAKER_24"},{"id":"320","startTime":2092.35,"endTime":2094.51,"text":"Don't reach for your waist!","speaker":"SPEAKER_24"},{"id":"321","startTime":2094.58,"endTime":2096.57,"text":"Do that and you're dead.","speaker":"SPEAKER_24"},{"id":"322","startTime":2097.32,"endTime":2101.65,"text":"The Reds treat POWs well.\nI've been one before.","speaker":"SPEAKER_24"},{"id":"323","startTime":2102.02,"endTime":2104.89,"text":"Just put those hands up.","speaker":"SPEAKER_24"},{"id":"331","startTime":2226.38,"endTime":2228.85,"text":"Where did you find this coat?","speaker":"SPEAKER_24"},{"id":"333","startTime":2234.19,"endTime":2237.82,"text":"The coat's from my brother's unit.","speaker":"SPEAKER_24"}]}]