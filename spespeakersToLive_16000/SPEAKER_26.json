[{"speaker":"SPEAKER_26","totalSegmentos":48,"segmentosSRT":[{"id":"53","startTime":398.26,"endTime":400.95,"text":"Little bastards from big bastards grow.","speaker":"SPEAKER_26"},{"id":"54","startTime":402.29,"endTime":404.52,"text":"I learned all I know from you.","speaker":"SPEAKER_26"},{"id":"55","startTime":404.9,"endTime":409.36,"text":"How do you think we lost\nmost of what we had in the first place?","speaker":"SPEAKER_26"},{"id":"56","startTime":410.97,"endTime":414.53,"text":"You're the one they call big bastard, Xu,\nnot me!","speaker":"SPEAKER_26"},{"id":"59","startTime":424.95,"endTime":427.94,"text":"Mother, when you finish,\nsend him to his room.","speaker":"SPEAKER_26"},{"id":"135","startTime":861.79,"endTime":863.95,"text":"What are you doing?","speaker":"SPEAKER_26"},{"id":"137","startTime":866.99,"endTime":869.19,"text":"Sit down.","speaker":"SPEAKER_26"},{"id":"138","startTime":869.39,"endTime":873.92,"text":"- Sit down!\n- You planned all this, didn't you?","speaker":"SPEAKER_26"},{"id":"142","startTime":892.12,"endTime":893.34,"text":"I'll stake my life.","speaker":"SPEAKER_26"},{"id":"143","startTime":893.42,"endTime":896.48,"text":"Don't talk like that.","speaker":"SPEAKER_26"},{"id":"144","startTime":896.55,"endTime":900.22,"text":"Anyway, your life's worth little now.","speaker":"SPEAKER_26"},{"id":"330","startTime":2223.18,"endTime":2225.27,"text":"Lao Quan! They're shooting!","speaker":"SPEAKER_26"},{"id":"335","startTime":2249.07,"endTime":2251.73,"text":"What's your brother look like?","speaker":"SPEAKER_26"},{"id":"336","startTime":2251.81,"endTime":2253.27,"text":"Just like me.","speaker":"SPEAKER_26"},{"id":"337","startTime":2271.79,"endTime":2273.02,"text":"Lao Quan!","speaker":"SPEAKER_26"},{"id":"517","startTime":3560.68,"endTime":3563.28,"text":"You won't go?","speaker":"SPEAKER_26"},{"id":"519","startTime":3569.86,"endTime":3574.32,"text":"Leave him alone! I'll kill him!","speaker":"SPEAKER_26"},{"id":"520","startTime":3574.4,"endTime":3575.33,"text":"Enough!","speaker":"SPEAKER_26"},{"id":"521","startTime":3581.5,"endTime":3582.99,"text":"How could you?","speaker":"SPEAKER_26"},{"id":"526","startTime":3596.55,"endTime":3598.58,"text":"I hit him for not obeying.","speaker":"SPEAKER_26"},{"id":"527","startTime":3598.65,"endTime":3600.02,"text":"That's no excuse.","speaker":"SPEAKER_26"},{"id":"528","startTime":3600.65,"endTime":3602.14,"text":"You spoil him.","speaker":"SPEAKER_26"},{"id":"553","startTime":3815.16,"endTime":3816.82,"text":"He wouldn't come.","speaker":"SPEAKER_26"},{"id":"554","startTime":3817.93,"endTime":3819.42,"text":"Ask him again.","speaker":"SPEAKER_26"},{"id":"555","startTime":3819.53,"endTime":3821.19,"text":"Say I want him to.","speaker":"SPEAKER_26"},{"id":"570","startTime":3955.93,"endTime":3958.33,"text":"Good boy, bring it over here.","speaker":"SPEAKER_26"},{"id":"571","startTime":3968.24,"endTime":3969.94,"text":"Daddy.","speaker":"SPEAKER_26"},{"id":"591","startTime":4135.96,"endTime":4138.36,"text":"Xia, Youqing, have some dumplings.","speaker":"SPEAKER_26"},{"id":"631","startTime":4387.04,"endTime":4388.67,"text":"It's Fugui's son?","speaker":"SPEAKER_26"},{"id":"641","startTime":4458.85,"endTime":4460.44,"text":"What's happened?","speaker":"SPEAKER_26"},{"id":"642","startTime":4462.08,"endTime":4463.35,"text":"Answer me!","speaker":"SPEAKER_26"},{"id":"643","startTime":4466.02,"endTime":4469.46,"text":"Daddy's here! Say something!","speaker":"SPEAKER_26"},{"id":"644","startTime":4482.57,"endTime":4484.54,"text":"Take the body away!","speaker":"SPEAKER_26"},{"id":"645","startTime":4485.37,"endTime":4487.57,"text":"We mustn't let his mother see.","speaker":"SPEAKER_26"},{"id":"646","startTime":4495.82,"endTime":4498.22,"text":"Mommy's here! Youqing!","speaker":"SPEAKER_26"},{"id":"680","startTime":4769.96,"endTime":4771.69,"text":"I had only one son!","speaker":"SPEAKER_26"},{"id":"681","startTime":4773.06,"endTime":4776.73,"text":"And you killed him!","speaker":"SPEAKER_26"},{"id":"682","startTime":4779.8,"endTime":4782,"text":"You, behind the goddamn wheel!","speaker":"SPEAKER_26"},{"id":"799","startTime":5398.49,"endTime":5400.89,"text":"Comrade, could I look at that one?","speaker":"SPEAKER_26"},{"id":"823","startTime":5500.96,"endTime":5502.92,"text":"Do stay! Come on inside.","speaker":"SPEAKER_26"},{"id":"855","startTime":5709.96,"endTime":5712.23,"text":"Time for a photo!","speaker":"SPEAKER_26"},{"id":"856","startTime":5718.87,"endTime":5720.84,"text":"Move aside! Stand still!","speaker":"SPEAKER_26"},{"id":"1154","startTime":7164.55,"endTime":7167.02,"text":"We'll see them soon.","speaker":"SPEAKER_26"},{"id":"1157","startTime":7172.66,"endTime":7175.29,"text":"- How is he?\n- They're both fine!","speaker":"SPEAKER_26"},{"id":"1158","startTime":7175.36,"endTime":7177.69,"text":"Erxi, you're a lucky man!","speaker":"SPEAKER_26"},{"id":"1189","startTime":7324.84,"endTime":7326.31,"text":"He's eaten too much.","speaker":"SPEAKER_26"},{"id":"1190","startTime":7326.38,"endTime":7328.04,"text":"What do you mean?","speaker":"SPEAKER_26"},{"id":"1191","startTime":7328.18,"endTime":7330.95,"text":"He gobbled up seven buns.","speaker":"SPEAKER_26"}]}]